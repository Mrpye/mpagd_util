	SPRITEINK 1
	LET SETTINGA = 0      ; reset the timer
	LET SETTINGB = 0      ; reset the timer
	LET FRAME = 0
ENDIF

;PLATFORM MODE
IF DIRECTION = 1 
	SPRITEINK 1
	ADD 1 TO SETTINGA      
	IF SETTINGA = 20
		LET SETTINGA = 0 
		LET DIRECTION = 2 
		LET FRAME = 2
	ENDIF
ENDIF

