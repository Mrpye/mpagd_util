	LET SETTINGA =50
ENDIF

; ******************
; THIS IS THE SWITCH
; ******************
IF IMAGE = 16
	IF SETTINGB = 1
		LET SETTINGA =0
	ENDIF
	IF SETTINGA = 20
		SPRITEINK 5 ; SET THE SPRITE COLOR
		LET O = 1
		LET FRAME =0
	ENDIF
	IF O = 0
		LET O = 1
	ENDIF
ELSE
	ANIMATE SLOW
	SPRITEINK 6 ; SET THE SPRITE COLOR
	; IF THE SWITCH IS PRESENT
	IF O = 1 ;SWITCH IS PRESENT BUT OFF
		LET SETTINGA = 0
	ENDIF
	IF SETTINGA < 50
		IF O = 2 ;SWITCH IS PRESENT BUT OFF
			LET SETTINGA = 40
		ENDIF
	ENDIF
ENDIF


IF SETTINGA = 40

	;SEE IF THE SWITCH IS OFF DO NOTHING
	IF O = 1
		LET SETTINGA = 0
		EXIT
	ELSE
		IF IMAGE <> 16
			LET SETTINGA = 1
			; KEEP LOOPING UNTIL FIND A VALID DIRECTION
			WHILE SETTINGA = 1
				ADD 1 TO DIRECTION        ; increment the frame
				IF DIRECTION = 4
					LET DIRECTION = 0
				ENDIF
				; TEST IF THEY CAN BE SENT THAT DIRECTION
				IF DIRECTION = UP
					IF CANGOUP
						LET SETTINGA = 0
					ENDIF	
				ENDIF
				IF DIRECTION = DOWN
					IF CANGODOWN
						LET SETTINGA = 0		
					ENDIF	
				ENDIF
				IF DIRECTION = LEFT
					IF CANGOLEFT
						LET SETTINGA = 0		
					ENDIF
				ENDIF
				IF DIRECTION = RIGHT
					IF CANGORIGHT
						LET SETTINGA = 0
					ENDIF	
				ENDIF
			ENDWHILE
		
		
			; SET THE IMAGE
			LET I = DIRECTION
			ADD 9 TO I
			LET IMAGE = I
			
			IF O = 2
				LET SETTINGA = 50
			ELSE
				LET SETTINGA = 0      
			ENDIF
		ENDIF
	ENDIF
	
ENDIF



IF COLLISION PLAYER
	; IGNORE COLLSION TO ALLOW TO PASS THROUGH
	IF SETTINGB = 1
		EXIT
	ENDIF
	
	; ******************
	; THIS IS THE SWITCH
	; ******************
	IF IMAGE = 16
		IF SETTINGB = 0
			IF FRAME =0
				LET FRAME = 1
				LET O = 2
				SPRITEINK 4 ; SET THE SPRITE COLOR
				LET SETTINGA = 0
				LET SETTINGB = 1 ;ACTIVATE COLLISION
				ENDIF
			EXIT
		ENDIF
	ENDIF

	
	
	; STORE THE SPRITE INFO
	LET V = X
	LET W = Y
	LET Q = DIRECTION
	LET SETTINGB = 1 ;IGNORE COLLISION
	
	; HANDLE THE PLAYER SPRITE
	OTHER
		LET X = V
		LET Y = W
		LET DIRECTION = Q
		LET IMAGE = DIRECTION
	ENDSPRITE

	
ELSE
	LET SETTINGB = 0 ;ACTIVATE COLLISION
ENDIF

