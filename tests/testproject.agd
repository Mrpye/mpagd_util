IF DIRECTION = 2
	SPRITEINK 2
	
	ADD 1 TO SETTINGA  
	IF SETTINGA = 5
		; ANIMATE BLADES
		LET SETTINGA = 0 
		IF FRAME = 2
			LET FRAME = 3
		ELSE
			LET FRAME = 2 
		ENDIF
		IF SETTINGB = 1
			;IF PLAYER IS ON PLATFORM THEN MARK TO KILL
			LET G = X
			ADD Y TO G
			IF T = G
				LET T = 2
			ENDIF
		ENDIF
		ADD 1 TO SETTINGB
		; CHECK IF TIME TO TURN TO PLATFORM
		IF SETTINGB = 12
			LET SETTINGA = 0
			LET SETTINGB = 0
			LET DIRECTION = 0
			LET FRAME = 0
			;IF T > 0
				;LET T = 1
			;ENDIF
		ENDIF
	ENDIF
ENDIF



