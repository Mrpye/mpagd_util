EVENT SPRITETYPE1
;Fire Ball Left Right
SPRITEINK 6
IF DIRECTION = 0            ; INITIALISE THE DRIP
	LET SETTINGB = X         ; store initial y value in SETTINGB
	LET DIRECTION = 1        ; prevent this running again
	LET SETTINGA = 0
ENDIF

IF DIRECTION = 1            ; GROW THE DRIP
	ADD 1 TO SETTINGA        ; we use SA as the timer
	LET R = 10
	IF FRAME = 0
		LET R = X
	ENDIF
	IF SETTINGA = R         ; after 2/5ths of a second
		ADD 1 TO FRAME        ; increment the frame
		LET SETTINGA = 0      ; reset the timer
		IF FRAME = 3          ; have we reached frame 4?
			LET DIRECTION = 2  ; if so goto the drop routine
			USER 5
		ENDIF
   ENDIF
ENDIF

IF DIRECTION = 2            ; DROP ACID!
	LET SETTINGA = 2
	WHILE SETTINGA>0
		SUBTRACT 1 FROM SETTINGA
		IF IMAGE = 4
			IF CANGOLEFT
				SPRITELEFT
				IF X < LEFTEDGE
					LET SETTINGA = 0
					LET DIRECTION = 3
					ADD 1 TO FRAME 
				ENDIF
			ELSE
				LET SETTINGA = 0
				LET DIRECTION = 3
				ADD 1 TO FRAME 
			ENDIF
		ELSE
			IF CANGORIGHT
				SPRITERIGHT
				IF X > RIGHTEDGE
					LET SETTINGA = 0
					LET DIRECTION = 3
					ADD 1 TO FRAME 
				ENDIF
			ELSE
				LET SETTINGA = 0
				LET DIRECTION = 3
				ADD 1 TO FRAME 
			ENDIF
		ENDIF
	ENDWHILE
ENDIF

IF DIRECTION = 3       
   ADD 1 TO SETTINGA        ; we use SA as the timer
   IF SETTINGA = 5         ; after 2/5ths of a second
		IF FRAME = 5          ; have we reached frame 4?
			LET X = SETTINGB   ; reset the drop
			LET DIRECTION = 1  ; start the INITALISE DRIP routine
			LET FRAME = 0 
			LET SETTINGA = 0
		ELSE
			ADD 1 TO FRAME        ; increment the frame
			LET SETTINGA = 0      ; reset the timer
		ENDIF
   ENDIF
ENDIF
IF FRAME > 0
	SPRITEINK 2
ENDIF

IF COLLISION PLAYER         ; CHECK COLLISION PLAYER
	IF FRAME > 0 ;FIRE BALL ACTIVE        
		OTHER	
			LET K = 50
			REMOVE                ; remove this sprite
			EXPLODE 24  
		ENDSPRITE
	ENDIF
ENDIF
