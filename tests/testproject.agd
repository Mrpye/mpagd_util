			EXPLODE 24  
		ENDSPRITE
		EXIT
	ENDIF

	; STAND ON THE PLATFORM
	
	OTHER		
		LET X = V
		LET Y = W
		LET G = X
		ADD Y TO G
		LET T = G ; SET T FLAG TO SAY STANDING ON PLATFORM
		IF DIRECTION = RIGHT
			SUBTRACT 16 FROM X
			LET IMAGE = LEFT
		ENDIF
		IF DIRECTION = LEFT
			ADD 16 TO X
			LET IMAGE = RIGHT
		ENDIF
		IF DIRECTION = DOWN
			SUBTRACT 16 FROM Y
			LET IMAGE = UP
		ENDIF
		IF DIRECTION = UP
			ADD 16 TO Y
			LET IMAGE = DOWN
		ENDIF
		LET SETTINGA = 0
		LET DIRECTION = 5
	ENDSPRITE
	;USER 1
	LET FRAME = 1
	LET DIRECTION = 1
	
ENDIF




